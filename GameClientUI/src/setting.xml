		<filterArr id="labelFilterBlack" desc="黑色滤镜(浅)">
			<filter filterType="GlowFilter" alpha="0.40" blurY="2" color="0x000000" quality="1" blurX="2" inner="false" strength="4" knockout="false" />
		</filterArr>
		
		<filterArr  id="textFilterYellow" desc="黄色滤镜">
