		</filterArr>
		<filterArr  id="textFilterGreen" desc="绿色滤镜">
			<filter filterType="GlowFilter" alpha="1.00" blurY="4" color="0x00FF00" quality="1" blurX="4" inner="false" strength="10" knockout="false"/>
