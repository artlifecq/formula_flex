<root>
	<defalutTextColor>0xFFFFFF</defalutTextColor>
	<defalutFontSize>14</defalutFontSize>
	<defalutFontFamily>SimHei</defalutFontFamily>
	<!-- <defalutFontFamily>微软雅黑</defalutFontFamily> -->
	<!--  是否启用，label的 自动缩略功能-->
	<truncateToFit>0</truncateToFit>
	<!-- 最近打开文件的，最大保存个数-->
	<maxSaveHistoryOpenFileNum>5</maxSaveHistoryOpenFileNum>
	<scaleUseRepeate>1</scaleUseRepeate>
	<textrueCopyFileType>*.png.atf *.inf *.png.bpg *.png</textrueCopyFileType>
	<!-- 需同步的素材目录（相对project\resource\ui）-->
    <uiAssetsCopyDir>big_bg</uiAssetsCopyDir>
	
	<repeatScale9SkinMiniWidth>20</repeatScale9SkinMiniWidth>
	<repeatScale9SkinMiniHeight>20</repeatScale9SkinMiniHeight>
	
	<filters>
		<filterArr id="textFilterRedGreen" desc="红绿滤镜">
			<filter filterType="GlowFilter" alpha="1.00" blurY="4" color="0xFF0000" quality="1" blurX="4" inner="false" strength="10" knockout="false" />
			<filter filterType="GlowFilter" alpha="1.00" blurY="4" color="0x00FF00" quality="1" blurX="4" inner="false" strength="10" knockout="false"/>
		</filterArr>
		<filterArr id="textFilterRed" desc="红色滤镜">
