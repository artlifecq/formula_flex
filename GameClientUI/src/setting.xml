		<filterArr id="labelFilterBlack" desc="黑色滤镜(浅)">
			<filter filterType="GlowFilter" alpha="1.00" blurY="1.4" color="0x000000" quality="1" blurX="1.4" inner="false" strength="10" knockout="false" />
		</filterArr>
		
		<filterArr  id="textFilterYellow" desc="黄色滤镜">
			<filter filterType="GlowFilter" alpha="1.00" blurY="2" color="0xFFFF00" quality="1" blurX="2" inner="false" strength="10" knockout="false"/>
		</filterArr>
		
		
		<filterArr  id="labelFilterBlack2" desc="黑色滤镜">
			<filter filterType="GlowFilter" alpha="1" blurY="2" color="0x000000" quality="1" blurX="2" inner="false" strength="10" knockout="false"/>
		</filterArr>
		<filterArr  id="labelFilterBlue" desc="黄色滤镜">
			<filter filterType="GlowFilter" alpha="0.70" blurY="10" color="0xc0710a" quality="1" blurX="10" inner="false" strength="2" knockout="false"/>
		</filterArr>
		
		
		<filterArr  id="textFilterBlackGreen" desc="墨绿滤镜(浅)">
			<filter filterType="GlowFilter" alpha="1.00" blurY="2" color="0x000000" quality="1" blurX="2" inner="false" strength="10" knockout="false"/>
		</filterArr>
		<filterArr  id="textFilterBlackGreen2" desc="墨绿滤镜(淡)">
			<filter filterType="GlowFilter" alpha="1.00" blurY="4" color="0x0f1a04" quality="1" blurX="4" inner="false" strength="10" knockout="false"/>
		</filterArr>
		<filterArr  id="textFilterBlackGreen3" desc="墨绿滤镜(暗)">
			<filter filterType="GlowFilter" alpha="1.00" blurY="2" color="0x0f1a04" quality="1" blurX="4" inner="false" strength="10" knockout="false"/>
		</filterArr>
		
